{
  "author": "D3b-TIRU",
  "cite": "",
  "command": "python run.py",
  "config": {
    "debug": {
      "default": false,
      "description": "Log debug messages",
      "type": "boolean"
    }
  },
  "custom": {
    "flywheel": {
      "suite": "Curation"
    },
    "gear-builder": {
      "category": "analysis",
      "image": "chop.flywheel.io/d3b-convert-nii2dcm:0.0.1"
    }
  },
  "environment": {
    "PATH": "/opt/poetry/bin:/usr/local/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "PYTHON_VERSION": "3.9.7",
    "PIP_NO_CACHE_DIR": "0",
    "POETRY_HOME": "/opt/poetry",
    "POETRY_VERSION": "1.1.11",
    "POETRY_VIRTUALENVS_CREATE": "false",
    "FLYWHEEL": "/flywheel/v0"
  },
  "inputs": {
    "api-key": {
      "base": "api-key"
    }
  },
  "label": "Convert nii2dcm (project-level)",
  "description": "Convert nifti files 2 DICOM (project-level)",
